fn main() {
    out("=== Testing Built-in Type Conversion Functions ===")
    
    # Test str() function
    out("Testing str():")
    age = 25
    out(str(age))
    
    # Test int() function  
    out("Testing int():")
    out(str(int(42)))
    pi = 3.14
    out(str(int(pi)))
    
    # Test flt() function
    out("Testing flt():")
    out(str(flt(42)))
    out(str(flt(3.14)))
    
    out("Type conversion tests completed!")
}