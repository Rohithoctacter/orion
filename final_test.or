fn main() {
    out("=== Final Test of Built-in Type Conversion Functions ===")
    
    # Test str() conversions
    out("str(25):")
    out(str(25))
    
    out("str(3.14):")
    out(str(3.14))
    
    # Test int() conversions
    out("int(42):")
    out(int(42))
    
    out("int(3.14):")
    out(int(3.14))
    
    # Test flt() conversions
    out("flt(42):")
    out(flt(42))
    
    out("flt(3.14):")
    out(flt(3.14))
    
    # Test nested conversions
    out("str(int(3.14)):")
    out(str(int(3.14)))
    
    out("str(flt(42)):")
    out(str(flt(42)))
    
    out("All tests completed!")
}