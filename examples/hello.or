// Hello World example in Orion
// Demonstrates basic function definition and string output

fn main() {
    out("Hello, Orion World!")
    out("Welcome to the fast and readable programming language!")
    
    // Variable declaration with type inference
    name = "Developer"
    out("Hello, " + name + "!")
    
    // Explicit type declaration
    int year = 2025
    out("Current year: " + str(year))
}
