# Test Python-style dictionaries in Orion
fn main() {
    out("TEST 1: Basic Dictionary Creation")
    person = {"name": "Alice", "age": "30"}
    out("Dictionary created successfully")
    
    out("TEST 2: Dictionary with Mixed Types")
    scores = {"math": "95", "english": "87"}
